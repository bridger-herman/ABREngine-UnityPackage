{
    "name": "edu.umn.cs.ivlab.abrengine",
    "displayName": "ABR Engine",
    "version": "2022.09",
    "unity": "2020.3",
    "description": "Artifact-Based Rendering Engine",
    "documentationUrl": "https://ivlab.github.io/ABREngine-UnityPackage/",
    "dependencies": {
        "com.unity.textmeshpro": "3.0.6"
    },
    "samples": [
        {
            "displayName": "State Creation",
            "description": "Contains a script to manually create a state using commands from the ABREngine.",
            "path": "Samples~/CreateState"
        },
        {
            "displayName": "Interactive state loading and modification",
            "description": "Contains a script to load a state from a JSON file, then modify it interactively every frame (in essence creating an animation)",
            "path": "Samples~/InteractiveState"
        },
        {
            "displayName": "Dataset Creation",
            "description": "Contains a script to manually create and load a raw ABR dataset",
            "path": "Samples~/CreateDataset"
        },
        {
            "displayName": "ABR Testing",
            "description": "If you are contributing to the ABREngine, you may use this script as a framework to test your new features.",
            "path": "Samples~/TestingABR"
        }
    ]
}
